    {
      "datasource": { "type": "prometheus", "uid": "${ds_prometheus}" },
      "description": "Real-time cost burn rate showing how fast spending is accumulating.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          },
          "unit": "currencyUSD"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 5, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": true,
        "textMode": "auto",
        "wideLayout": true
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${ds_prometheus}" },
          "expr": "sum(rate(claude_code_cost_usage_USD_total[$__rate_interval]))",
          "legendFormat": "Burn Rate",
          "refId": "A"
        }
      ],
      "title": "Real-Time Cost Burn Rate",
      "type": "stat"
    },
